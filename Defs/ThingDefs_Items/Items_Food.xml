<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
	<!-- Basic Food -->
	<!-- Cookie -->
	<ThingDef ParentName="MealCooked">
		<defName>DankPyon_Cookie</defName>
		<label>cookie</label>
		<description>A delightful treat baked to perfection, offering a moment of comfort and a burst of sugary energy for your colonists.</description>
		<graphicData>
			<texPath>Meal/Cookie</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>75</MarketValue>
			<WorkToMake>1100</WorkToMake>
			<Nutrition>1</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealSimple</preferability>
			<tasteThought>DankPyon_AteCookie</tasteThought>
			<optimalityOffsetHumanlikes>18</optimalityOffsetHumanlikes>
			<optimalityOffsetFeedingAnimals>-30</optimalityOffsetFeedingAnimals>
			<outcomeDoers>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>DankPyon_AteBake</hediffDef>
					<severity>1</severity>
				</li>
			</outcomeDoers>
		</ingestible>
		<rotatable>false</rotatable>
	</ThingDef>
	
